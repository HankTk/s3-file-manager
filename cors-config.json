{
    "CORSRules": [
        {
            "AllowedHeaders": [
                "*",
                "Authorization",
                "Content-Type",
                "x-amz-date",
                "x-amz-content-sha256",
                "x-amz-user-agent",
                "x-amz-acl"
            ],
            "AllowedMethods": [
                "GET",
                "PUT",
                "POST",
                "DELETE",
                "HEAD"
            ],
            "AllowedOrigins": [
                "http://localhost:4200",
                "http://127.0.0.1:4200"
            ],
            "ExposeHeaders": [
                "ETag",
                "x-amz-server-side-encryption",
                "x-amz-request-id",
                "x-amz-id-2"
            ],
            "MaxAgeSeconds": 3000
        }
    ]
} 